{
  "name": "js-utils",
  "version": "1.0.0",
  "description": "Utility functions for JS",
  "main": "utils.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {},
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "prettier": "^1.15.3"
  },
  "scripts": {
    "test": "nyc mocha; exit 0"
  },
  "nyc": {
    "skip-full": true,
    "reporter": [
      "html",
      "text-summary"
    ]
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "printWidth": 90,
    "proseWrap": "preserve",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lowderl/js-utils.git"
  },
  "keywords": [
    "js",
    "javascript",
    "util",
    "utils",
    "utilities"
  ],
